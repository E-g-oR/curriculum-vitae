{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"],
      "@pages/*": ["src/pages/*"],
      "@content/*": ["src/content/*"],
      "@i18n/*": ["src/i18n/*"],
      "@utils/*": ["src/utils/*"],
    }
  }
}
